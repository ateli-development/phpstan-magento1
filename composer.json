{
    "name": "ateli-development/phpstan-magento1",
    "description": "Extension for PHPStan to allow analysis of Magento 1 code.",
    "type": "library",
    "require": {
        "phpstan/phpstan": "^0.12",
        "php": ">= 7.2"
    },
    "replace": {
        "vianetz/phpstan-magento1": "dev-compatibility_with_phpstan_v0.12"
    },
    "autoload": {
        "psr-4": {
            "PHPStanMagento1\\": "src/"
        }
    },
    "scripts": {
        "test-quality": [
            "phpstan analyse"
        ],
        "test": [
            "@test-quality"
        ]
    },
    "license": "MIT"
}
